{
  "name": "svelte-app",
  "version": "1.0.0",
  "scripts": {
    "build": "cross-env NODE_ENV=production rollup -c",
    "dev1": "nodemon --watch src -e js,ts,html,svelte --exec \"npm run build\"",
    "dev": "rollup -c -w",
    "server": "npm run server:dev",
    "server:dev": "npm run server:start -- --dev --single --cors --host --port 8000",
    "server:start": "sirv public",
    "clean": "npx rimraf -rf public",
    "start": "concurrently -k \"npm:dev\" \"npm:server:dev\"",
    "validate": "svelte-check"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^15.1.0",
    "@rollup/plugin-html": "^0.2.0",
    "@rollup/plugin-multi-entry": "^4.0.0",
    "@rollup/plugin-node-resolve": "^9.0.0",
    "@rollup/plugin-typescript": "^6.0.0",
    "@tsconfig/svelte": "^1.0.10",
    "concurrently": "^5.3.0",
    "cross-env": "^7.0.2",
    "nodemon": "^2.0.4",
    "rimraf": "^3.0.2",
    "rollup": "^2.28.2",
    "rollup-plugin-generate-html-template": "^1.7.0",
    "rollup-plugin-livereload": "^2.0.0",
    "rollup-plugin-multi-input": "^1.1.1",
    "rollup-plugin-svelte": "^6.0.1",
    "rollup-plugin-terser": "^7.0.2",
    "svelte": "^3.28.0",
    "svelte-check": "^1.0.51",
    "svelte-preprocess": "^4.3.2",
    "tslib": "^2.0.1",
    "typescript": "^4.0.3"
  },
  "dependencies": {
    "rmdir": "^1.2.0",
    "sirv-cli": "^1.0.6"
  }
}
